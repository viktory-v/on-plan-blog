    .text {
      margin-top: -55px;

      h3 {
        line-height: 65px;
        margin-bottom: 10px;
      }

      p {
        line-height: 55px;
        margin-bottom: 53px;
      }
    }
  }

  @media (max-width: 750px) {
    .inner {
      padding: 29px 35px 40px;
    }

    .col {
      &:first-child {
        margin-top: 135px;

        &::before {
          display: none;
        }
      }

      &:last-child {
        max-width: 312px;
        margin-right: 18px;

        &::after {
          width: 288px;
          height: 335px;
          top: -18px;
          right: -18px;
        }

        &::before {
          display: none;
        }
      }
    }

    .img {
      margin-left: 0;
    }

    .title {
      display: none;
    }

    .text {
      h3 {
        font-size: 24px;
        line-height: 36px;
        margin-bottom: -1px;
      }

      span {
        font-size: 24px;
        line-height: 36px;
      }

      p {
        font-size: 24px;
        line-height: 36px;
        margin-bottom: 40px;
      }
    }
  }

  @media (max-width: 400px) {
    .inner {
      padding-left: 15px;
      padding-right: 15px;
    }

    .title {
      display: block;
      font-size: 80px;
      line-height: 120px;
      margin-left: 10px;
    }

    .col {
      &:first-child {
        margin-top: -8px;
      }
    }

    .text {
      margin-top: -35px;
    }
  }
}
