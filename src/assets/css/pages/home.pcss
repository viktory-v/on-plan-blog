@import '../global-mixins.pcss';

.section-hero {
  position: relative;
  overflow: hidden;
  padding-bottom: 140px;

  & > img {
    height: calc(100% - 100px);
    position: absolute;
    object-position: left;
    left: calc(50% + 185px);
    width: calc(50% - 187px);
    background-color: #e0f1fe;
  }

  .inner {
    display: flex;
    max-width: 1184px;
    padding-top: 105px;
    padding-bottom: 123px;
  }

  .text {
    padding-right: 40px;
    padding-left: 7px;

    .under-text {
      font-style: italic;
      font-weight: 600;
      font-size: 20px;
      line-height: 18px;
      color: #20a4f3;
      margin-bottom: 22px;
    }

    .title {
      font-style: normal;
      font-weight: bold;
      font-size: 50px;
      line-height: 75px;
      color: #000;
      margin-bottom: 20px;

      span {
        font-family: monospace;
        font-size: 63px;
        vertical-align: top;
        margin: 0 1px;
        transform: translateY(-2px);
      }
    }

    p {
      font-style: normal;
      font-weight: 300;
      font-size: 18px;
      line-height: 34px;
      color: #000;
      margin-bottom: 45px;
      padding-right: 30px;
    }
  }

  .col {
    width: 100%;

    &:first-child {
      &::after {
        position: absolute;
        content: '';
        background-image: url(../../images/bgi-vegetables.png);
        background-repeat: no-repeat;
        left: -448px;
        bottom: -155px;
        width: 100%;
        height: 875px;
        background-size: contain;
      }
    }

    &:last-child {
      &::before {
        content: '';
        position: absolute;
        background-image: url(../../images/brocolli.png);
        background-repeat: no-repeat;
        width: 89px;
        height: 101px;
        bottom: -33px;
        right: calc(46% - 5px);
        z-index: 1;
      }
    }
  }

  .img-list {
    display: flex;
    list-style: none;

    img {
      position: absolute;
    }

    li {
      &:nth-child(1) img {
        width: 524px;
        left: calc(39% - 7px);
        top: -28px;
      }

      &:nth-child(2) img {
        width: 517px;
        right: calc(-11% - -2px);
        top: calc(13% - -3px);
      }

      &:nth-child(3) img {
        width: 339px;
        top: calc(50% - 3px);
        left: calc(51% - -2px);
      }
    }
  }

  @media (max-width: 1280px) {
    .inner {
      padding-left: 0;
      padding-right: 0;
    }
  }

  @media (max-width: 1200px) {
    padding-bottom: 0;
    padding-top: 38px;

    & > img {
      left: 0;
      width: 100%;
      height: 410px;
      object-position: center 78%;
    }

    .inner {
      flex-direction: column-reverse;
      padding-left: 35px;
      padding-right: 35px;
      padding-bottom: 111px;
    }

    .col {
      &:first-child {
        margin-top: 337px;

        &::after {
          display: none;
        }
      }

      &:last-child {
        position: relative;

        &::before {
          display: none;
        }
      }
    }

    .img-list {
      position: relative;

      li {
        &:nth-child(1) img {
          width: 342px;
          left: calc(23% - 26px);
          top: -150px;
        }

        &:nth-child(2) img {
          width: 339px;
          left: calc(50% - 62px);
          top: -70px;
        }

        &:nth-child(3) img {
          width: 222px;
          top: calc(50% - -100px);
          left: calc(51% - 195px);
        }
      }
    }

    .text {
      padding: 0;

      p {
        margin-bottom: 30px;
      }
    }
  }

  @media (max-width: 1024px) {
    padding-top: 40px;
  }

  @media (max-width: 768px) {
    .inner {
      padding-left: 60px;
      padding-right: 56px;
      padding-top: 103px;
    }

    .col {
      &:first-child {
        margin-top: 339px;
      }
    }

    .text {
      p {
        padding-right: 0;
      }

      .title span {
        font-size: 60px;
        margin: 0;
      }
    }

    .img-list {
      li {
        &:nth-child(1) img {
          left: calc(23% - 109px);
        }

        &:nth-child(3) img {
          top: calc(50% - -102px);
          left: calc(51% - 191px);
        }
      }
    }
  }

  @media (max-width: 750px) {
    .inner {
      padding-left: 35px;
      padding-right: 35px;
    }

    .text .title span {
      font-size: 29px;
    }

    .object-fit.object-fit-cover {
      height: 148px;
      object-position: center -484px;
    }

    .img-list {
      li {
        &:nth-child(1) img {
          left: calc(23% - 18px);
          top: -164px;
          width: 207px;
        }

        &:nth-child(2) img {
          width: 204px;
          left: calc(50% - 37px);
          top: -114px;
        }

        &:nth-child(3) img {
          width: 135px;
          top: calc(50% - 11px);
          left: calc(51% - 119px);
        }
      }
    }

    .col {
      &:first-child {
        margin-top: 134px;
      }
    }

    .text {
      .under-text {
        font-size: 14px;
        line-height: 18px;
        margin-bottom: 11px;
      }

      .title {
        font-size: 24px;
        line-height: 36px;
      }

      p {
        font-size: 16px;
        line-height: 28px;
        padding-right: 5px;
        margin-bottom: 40px;
      }
    }
  }

  @media (max-width: 400px) {
    .object-fit.object-fit-cover {
      height: 148px;
      object-position: center;
    }

    .inner {
      padding-left: 15px;
      padding-right: 15px;
      padding-bottom: 55px;
    }

    .text p {
      padding-right: 0;
    }

    .img-list {
      li {
        &:nth-child(1) img {
          left: calc(23% - 84px);
        }

        &:nth-child(3) img {
          width: 136px;
          left: calc(51% - 116px);
        }
      }
    }
  }
}

.section-beginning {
  .inner {
    display: flex;
    max-width: 1170px;
