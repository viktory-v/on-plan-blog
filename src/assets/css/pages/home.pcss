    &:last-child {
      display: flex;
      justify-content: flex-end;
      padding-right: 30px;
      padding-top: 39px;

      &::before {
        position: absolute;
        content: '';
        background-color: #20a4f3;
        width: 470px;
        height: 540px;
        top: 9px;
        right: 1px;
      }
    }
