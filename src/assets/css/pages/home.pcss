  }

  @media (max-width: 1200px) {
    padding: 0;

    .inner {
      flex-direction: column-reverse;
      align-items: center;
      padding-left: 36px;
      padding-right: 34px;
      padding-bottom: 117px;
    }

    .text {
      max-width: none;
    }

    .col {
      &:first-child {
        width: 100%;
        margin-top: 56px;
      }

      &:last-child {
        margin-right: 39px;
        margin-top: 39px;
        padding: 0;

        &::before {
          top: -30px;
          right: -30px;
        }
      }
    }
  }

  @media (max-width: 975px) {
    .col {
      &:last-child {
        width: 470px;
      }
    }
  }

  @media (max-width: 768px) {
    .inner {
      padding-left: 60px;
      padding-right: 60px;
    }

    .col {
      &:last-child {
        margin-right: 31px;
      }
    }
  }

  @media (max-width: 750px) {
    .inner {
      padding-right: 35px;
      padding-left: 35px;
      padding-bottom: 60px;
    }

    .col {
      &:first-child {
        margin-top: 26px;
      }

      &:last-child {
        width: 310px;
        margin-right: 21px;
        margin-top: 24px;

        &::before {
          width: 310px;
          height: 356px;
          top: -19px;
          right: -20px;
        }
      }
    }

    .title {
      font-size: 24px;
      line-height: 36px;
      margin-bottom: 21px;
    }

    .text a {
      margin-top: 29px;

      &::after {
        margin-left: 9px;
      }
    }
  }

  @media (max-width: 400px) {
    .inner {
      padding-left: 15px;
      padding-right: 15px;
      padding-bottom: 55px;
    }

    .text {
      padding-left: 0;

      a {
        max-width: 231px;
        margin-top: 29px;

        &::after {
          min-width: 41px;
          margin: 0;
        }
      }
    }
  }
}

