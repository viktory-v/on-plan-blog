.section-photo {
  position: relative;
  height: 401px;

  &::before {
    content: '';
    position: absolute;
    background-image: url(../../images/section-about-page-peppers.png);
    background-repeat: no-repeat;
    width: 721px;
    height: 660px;
    left: calc(50% - 227px);
    bottom: calc(50% - 251px);
  }

  .inner {
    max-width: 1170px;
  }

  .img-list {
    list-style: none;
    display: flex;

    img {
      position: absolute;
      z-index: 2;
    }

    li {
      &:nth-child(1) img {
        width: 473px;
        left: 122px;
        top: 29px;
      }

      &:nth-child(2) img {
        width: 452px;
        left: calc(50% + -70px);
        top: 81px;
      }

      &:nth-child(3) img {
        top: calc(50% + 426px);
        right: calc(50% + -1px);
      }
    }
  }

  @media (max-width: 1280px) {
    .inner {
      padding-left: 0;
      padding-right: 0;
    }
  }

  @media (max-width: 1200px) {
    &::before {
      left: calc(50% - 159px);
      background-image: url(../../images/section-about-small-peppers.png);
      width: 528px;
      bottom: calc(50% - 444px);
    }

    .img-list {
      li {
        &:nth-child(1) img {
          width: 359px;
          left: 173px;
          top: 41px;
        }

        &:nth-child(2) img {
          width: 344px;
          left: calc(50% + -43px);
          top: 79px;
        }

        &:nth-child(3) img {
          top: calc(50% + 342px);
          right: calc(50% + -14px);
          width: 147px;
        }
      }
    }
  }

  @media (max-width: 800px) {
    &::before {
      bottom: calc(50% - 445px);
      left: calc(50% - 169px);
    }

    .img-list {
      li {
        &:nth-child(1) img {
          left: 35px;
        }

        &:nth-child(2) img {
          left: calc(50% + -52px);
          top: 81px;
        }

        &:nth-child(3) img {
          right: calc(50% + -4px);
        }
      }
    }
  }

  @media (max-width: 720px) {
    height: 93px;

    &::before {
      background-image: url(../../images/Peppers-on-640.png);
      width: 242px;
      height: 179px;
      bottom: calc(50% - 96px);
      left: calc(50% - 92px);
    }

    .img-list {
      li {
        &:nth-child(1) img {
          left: 149px;
          top: -42px;
          width: 160px;
        }

        &:nth-child(2) img {
          left: calc(50% + -39px);
          top: -26px;
          width: 155px;
        }

        &:nth-child(3) img {
          right: calc(50% + 13px);
          top: calc(50% + 93px);
          width: 65px;
        }
      }
    }
  }

  @media (max-width: 520px) {
    &::before {
      bottom: calc(50% - 92px);
      left: calc(50% - 91px);
    }

    .img-list {
      li {
        &:nth-child(1) img {
          left: calc(50% - 171px);
          top: -46px;
        }

        &:nth-child(2) img {
          top: -28px;
        }

        &:nth-child(3) img {
          right: calc(50% + 13px);
          top: calc(50% + 90px);
        }
      }
    }
  }
}

.section-prefooter {
  background-color: #f8f8f8;

  .inner {
    max-width: 1015px;
    padding-top: 263px;
    padding-bottom: 63px;
  }

  .text {
    h2 {
      font-family: Poppins;
      font-style: italic;
      font-weight: 600;
      font-size: 23px;
      line-height: 38px;
      text-align: center;
      color: #20a4f3;
      letter-spacing: 0.11px;
      margin-bottom: 17px;
    }

    p {
      font-family: Poppins;
      font-style: normal;
      font-weight: 300;
      font-size: 16px;
      line-height: 30px;
      text-align: center;
      color: #000;
      padding: 0 115px;
      margin-bottom: 0;
    }
  }

  .btn-wrapper {
    display: flex;
    justify-content: center;
    margin-top: 36px;

    .btn {
      margin-left: 15px;
      margin-right: 15px;
      line-height: 22px;
      padding: 19px 34px 19px 32px;
    }

    .btn-white {
      padding: 18px 35px 18px 39px;
      background-color: transparent;
      color: #00b159;
      border: 1px solid #00b159;
    }
  }

  @media (max-width: 1280px) {
    .inner {
      padding-left: 0;
      padding-right: 0;
    }
  }

  @media (max-width: 1200px) {
    .inner {
      padding-left: 35px;
      padding-right: 35px;
      padding-top: 131px;
    }

    .text {
      h2 {
        margin-bottom: 21px;
      }

      br {
        display: none;
      }

      p {
        padding: 0;
      }
    }

    .btn-wrapper {
      margin-top: 39px;
    }
  }

  @media (max-width: 800px) {
    .inner {
      padding-left: 60px;
      padding-right: 60px;
      padding-bottom: 62px;
    }
  }

  @media (max-width: 720px) {
    .inner {
      padding: 103px 35px 20px;
    }

    .btn-wrapper {
      .btn {
        margin-left: 9px;
        margin-right: 11px;
      }
    }
  }

  @media (max-width: 530px) {
    .inner {
      padding-left: 15px;
      padding-right: 15px;
      padding-top: 99px;
    }

    .btn-wrapper {
      flex-direction: column;
      align-items: center;

      .btn {
        width: 150px;
      }

      .btn-white {
        width: 190px;
        margin-top: 15px;
      }
    }
  }
}
